{
  "name": "react-fancy-tbx",
  "version": "0.1.0",
  "main": "dist/react-fancy-tbx.umd.cjs",
  "module": "dist/react-fancy-tbx.js",
  "types": "dist/react-fancy-tbx.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**/*.{js,mjs,d.ts,css}"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && sass src/lib/react-fancy-tbx.scss > dist/react-fancy-tbx.css",
    "preview": "vite preview",
    "report:package": "npm pack --loglevel notice 2>&1 >/dev/null | sed -e 's/^npm notice //' && rm *.tgz"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.6.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.0",
    "sass": "^1.54.1",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vite-plugin-dts": "^1.4.1"
  }
}
